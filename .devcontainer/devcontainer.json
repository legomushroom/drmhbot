{
  "dockerComposeFile": "docker-compose.yml",
  "service": "bot",
  "workspaceFolder": "/workspace",
  "extensions": ["esbenp.prettier-vscode", "GitHub.copilot", "ms-azuretools.vscode-docker", "ms-python.python"],
  "postAttachCommand": "poetry install",
  "settings": {
    "[python]": {
      "editor.rulers": [88]
    },
    "python.formatting.provider": "black"
  }
}
